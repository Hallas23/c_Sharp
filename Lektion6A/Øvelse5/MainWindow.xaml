<Window x:Class="Øvelse5.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Øvelse5"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Menu x:Name="MenuBar" HorizontalAlignment="Left" Height="30" VerticalAlignment="Top" Width="792">
            <MenuItem x:Name="MenuFile" Header="File" Height="30" Width="50">
                <MenuItem Header="Open" Click="OpenClick" />
                <MenuItem Header="Save as" Click="SaveClick" />
                <MenuItem Header="Exit" Click="ExitClick"/>
            </MenuItem>
            <MenuItem x:Name="Help" Header="Help" Height="30" Width="50">
                <MenuItem Header="About" Click="AboutWindow"/>
            </MenuItem>
        </Menu>
        <StatusBar HorizontalAlignment="Left" Height="32" VerticalAlignment="Top" Width="792" Margin="0,387,0,0">
            <StatusBarItem x:Name="StatusBarI" Content="StartUp"></StatusBarItem>
        </StatusBar>
    </Grid>
</Window>
