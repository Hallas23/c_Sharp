<Window x:Class="Øvelse6.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Øvelse6"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel HorizontalAlignment="Left" Height="440" VerticalAlignment="Top" Width="790" Margin="0,0,0,-21">
        <RadioButton x:Name="R1" HorizontalAlignment="Center" Click="Method"> </RadioButton>
        <RadioButton x:Name="R2" HorizontalAlignment="Center" Click="Method"> </RadioButton>
        <RadioButton x:Name="R3" HorizontalAlignment="Center" Click="Method"> </RadioButton>
        <RadioButton x:Name="R4" HorizontalAlignment="Center" Click="Method"> </RadioButton>
        <CheckBox x:Name="C1" HorizontalAlignment="Center" Click="Method"> </CheckBox>
        <CheckBox x:Name="C2" HorizontalAlignment="Center" Click="Method"></CheckBox>
        <CheckBox x:Name="C3" HorizontalAlignment="Center" Click="Method"></CheckBox>
        <CheckBox x:Name="C4" HorizontalAlignment="Center" Click="Method"></CheckBox>
        <StatusBar HorizontalAlignment="Center" VerticalAlignment="Bottom">
            <StatusBarItem x:Name="SBI" Content=""> </StatusBarItem>
        </StatusBar>
    </StackPanel>
</Window>
