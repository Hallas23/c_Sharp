<Window x:Class="Øvelse2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Øvelse2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label x:Name = "Name"  Content="{Binding Path=NameP}" HorizontalAlignment="Left" Margin="23,34,0,0" VerticalAlignment="Top"/>
        <Label x:Name = "Age"  Content="{Binding Path=AgeP}" HorizontalAlignment="Left" Margin="23,65,0,0" VerticalAlignment="Top"/>
        <Label x:Name = "Weight" Content="{Binding Path=WeightP}" HorizontalAlignment="Left" Margin="23,96,0,0" VerticalAlignment="Top"/>
        <Label x:Name = "Score"  Content="{Binding Path=ScoreP}" HorizontalAlignment="Left" Margin="23,127,0,0" VerticalAlignment="Top"/>
        <Label x:Name = "Accepted"  Content="{Binding Path=AcceptedP}" HorizontalAlignment="Left" Margin="23,158,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="TextBox1" Height="23" Margin="487,34,0,0" TextWrapping="Wrap" TextChanged="TextBox1_TextChanged" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button x:Name= "Button1" HorizontalAlignment="Left" VerticalAlignment="Top"  Width="74" Margin="580,81,0,0"/>

    </Grid>
</Window>
