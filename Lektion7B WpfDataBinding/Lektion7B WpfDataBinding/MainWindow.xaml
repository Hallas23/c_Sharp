<Window x:Class="Lektion7B_WpfDataBinding.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lektion7B_WpfDataBinding"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label x:Name = "Name"  Content="{Binding Path=NameP}" HorizontalAlignment="Left" Margin="23,34,0,0" VerticalAlignment="Top"/>
        <Label x:Name = "Age"  Content="{Binding Path=AgeP}" HorizontalAlignment="Left" Margin="23,65,0,0" VerticalAlignment="Top"/>
        <Label x:Name = "Weight" Content="{Binding Path=WeightP}" HorizontalAlignment="Left" Margin="23,96,0,0" VerticalAlignment="Top"/>
        <Label x:Name = "Score"  Content="{Binding Path=ScoreP}" HorizontalAlignment="Left" Margin="23,127,0,0" VerticalAlignment="Top"/>
        <Label x:Name = "Accepted"  Content="{Binding Path=AcceptedP}" HorizontalAlignment="Left" Margin="23,158,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
